#include<iostream>
#include<cstring>

using namespace std;

char alph[12] = { 'k','i','j','u','h','y','n','b','m','l','p','o' };

int solve()
{
	int N;
	string str;
	cin >> N;
	cin >> str;
	for (int i = 0; i < 12; i++)
	{
		if (str[N - 1] == alph[i])
			return 0;
	}
	return 1;
}

int main()
{
	cout << solve() << endl;
}
